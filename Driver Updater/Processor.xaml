<Page
    x:Class="Driver_Updater.Processor"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:local="clr-namespace:Driver_Updater"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    Title="Processor_motherboard_page       "
    d:DesignHeight="325"
    d:DesignWidth="425"
    mc:Ignorable="d">
    <Grid Background="#FF3B3B3B">
        <ScrollViewer
            Width="435"
            Height="330"
            HorizontalAlignment="Left"
            VerticalAlignment="Top"
            CanContentScroll="True">
            <StackPanel
                Width="405"
                Height="325"
                Orientation="Vertical">
                <StackPanel Margin="0,8,0,0" Orientation="Horizontal">
                    <TextBlock
                        Width="150"
                        Height="21"
                        Margin="0"
                        Padding="25,0,0,0"
                        HorizontalAlignment="Left"
                        VerticalAlignment="Top"
                        Foreground="#FFD8D8D8"
                        Text="Central Processor"
                        TextWrapping="Wrap">
                        <TextBlock.Background>
                            <ImageBrush ImageSource="13.png" Stretch="Uniform">
                                <ImageBrush.RelativeTransform>
                                    <TransformGroup>
                                        <ScaleTransform CenterX="0.5" CenterY="0.5" ScaleX="0.65" ScaleY="0.65" />
                                        <SkewTransform CenterX="0.5" CenterY="0.5" />
                                        <RotateTransform CenterX="0.5" CenterY="0.5" />
                                        <TranslateTransform X="-0.45" />
                                    </TransformGroup>
                                </ImageBrush.RelativeTransform>
                            </ImageBrush>
                        </TextBlock.Background>
                    </TextBlock>
                </StackPanel>
                <StackPanel Margin="0,8,0,0" Orientation="Horizontal">
                    <TextBlock
                        Width="130"
                        Height="21"
                        Margin="0"
                        Padding="25,0,0,0"
                        HorizontalAlignment="Left"
                        VerticalAlignment="Top"
                        Foreground="#FFD8D8D8"
                        TextWrapping="Wrap">
                        <TextBlock.Background>
                            <ImageBrush ImageSource="14.png" Stretch="Uniform">
                                <ImageBrush.RelativeTransform>
                                    <TransformGroup>
                                        <ScaleTransform CenterX="0.5" CenterY="0.5" ScaleX="0.65" ScaleY="0.65" />
                                        <SkewTransform CenterX="0.5" CenterY="0.5" />
                                        <RotateTransform CenterX="0.5" CenterY="0.5" />
                                        <TranslateTransform X="-0.45" />
                                    </TransformGroup>
                                </ImageBrush.RelativeTransform>
                            </ImageBrush>
                        </TextBlock.Background><Run Text="CPU Name" /><LineBreak /><Run /></TextBlock>
                    <TextBlock
                        x:Name="CPUName"
                        Width="200"
                        Height="40"
                        Margin="25,0,0,0"
                        HorizontalAlignment="Left"
                        VerticalAlignment="Top"
                        Foreground="#FFD8D8D8"
                        TextWrapping="Wrap" />
                </StackPanel>
                <StackPanel Margin="0,8,0,0" Orientation="Horizontal">
                    <TextBlock
                        Width="130"
                        Height="43"
                        Margin="0"
                        Padding="25,0,0,0"
                        HorizontalAlignment="Left"
                        VerticalAlignment="Top"
                        Foreground="#FFD8D8D8"
                        Text="Number of Logical Processors"
                        TextWrapping="Wrap">
                        <TextBlock.Background>
                            <ImageBrush ImageSource="15.png" Stretch="Uniform">
                                <ImageBrush.RelativeTransform>
                                    <TransformGroup>
                                        <ScaleTransform CenterX="0.5" CenterY="0.5" ScaleX="0.6" ScaleY="0.6" />
                                        <SkewTransform CenterX="0.5" CenterY="0.5" />
                                        <RotateTransform CenterX="0.5" CenterY="0.5" />
                                        <TranslateTransform X="-0.43" />
                                    </TransformGroup>
                                </ImageBrush.RelativeTransform>
                            </ImageBrush>
                        </TextBlock.Background>
                    </TextBlock>
                    <TextBlock
                        x:Name="NumberOfCores"
                        Width="200"
                        Height="20"
                        Margin="20,0,0,0"
                        HorizontalAlignment="Left"
                        VerticalAlignment="Top"
                        Foreground="#FFD8D8D8"
                        TextWrapping="Wrap"><Run Text="sssss" /><LineBreak /><Run /></TextBlock>
                </StackPanel>
                <StackPanel Margin="0,8,0,0" Orientation="Horizontal">
                    <TextBlock
                        Width="130"
                        Height="21"
                        Margin="0"
                        HorizontalAlignment="Left"
                        VerticalAlignment="Top"
                        Foreground="#FFD8D8D8"
                        TextWrapping="Wrap"><Run Text="Manufacturer" /><LineBreak /><Run /></TextBlock>
                    <TextBlock
                        x:Name="Manufacturer"
                        Width="200"
                        Height="20"
                        Margin="20,0,0,0"
                        HorizontalAlignment="Left"
                        VerticalAlignment="Top"
                        Foreground="#FFD8D8D8"
                        Text="sample"
                        TextWrapping="Wrap" />
                </StackPanel>
                <StackPanel Margin="0,8,0,0" Orientation="Horizontal">
                    <TextBlock
                        Width="130"
                        Height="21"
                        Margin="0"
