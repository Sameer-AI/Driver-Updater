                        Height="21"
                        Margin="0"
                        HorizontalAlignment="Left"
                        VerticalAlignment="Top"
                        Foreground="#FFD8D8D8"
                        Text="PCI is Supported"
                        TextWrapping="Wrap" />
                    <TextBlock
                        x:Name="PCISupport"
                        Width="200"
                        Height="20"
                        Margin="20,0,0,0"
                        HorizontalAlignment="Left"
                        VerticalAlignment="Top"
                        Foreground="#FFD8D8D8"
